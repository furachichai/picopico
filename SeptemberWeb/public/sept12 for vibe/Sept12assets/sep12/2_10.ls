global oGproperty SAMBIENT,STURN,SMISSILE,SEXPLOTION,SCRY,SCLICK,SMAX,PEOPLE,array,countcryon new me  me.SAMBIENT=1  me.STURN=2  me.SCLICK=3    me.SMISSILE=4  me.SEXPLOTION=5  me.SCRY=6    me.SMAX=6    me.PEOPLE=50    me.countcry=0  me.array=[]  me.array.append([#thesound:"Ambient",#thefadein:1,#thefadeout:300,#vol:50])  me.array.append([#thesound:"Turn",#thefadein:1,#thefadeout:300,#vol:70])  me.array.append([#thesound:"Click",#thefadein:1,#thefadeout:300,#vol:75])    me.array.append([#thesound:"Missile",#thefadein:1,#thefadeout:300,#vol:200])  me.array.append([#thesound:"Explotion",#thefadein:1,#thefadeout:300,#vol:215])  me.array.append([#thesound:"Cry",#thefadein:1,#thefadeout:300,#vol:50])    me.mSetVolumes()  return meendon mSetVolumes me  repeat with i=1 to me.SMAX    sound(i).volume = me.array[i].vol  end repeat  endon mPlay me,thesound    if not sound(thesound).isBusy()  then    puppetSound thesound, me.array[thesound].thesound    sound(thesound).play()      end if  endon mPlayPeople me,thesound    puppetSound thesound, me.array[thesound].thesound    if sound(thesound).isBusy() then      if sound(thesound).currentTime>(member(me.array[thesound].thesound).duration/3) then      sound(thesound).play()    end if      else    sound(thesound).play()          end if  endon mPlayCry me  if me.countcry=0 then    me.mPlay(me.SCRY)  end if    me.countcry=me.countcry+1  endon mStopCry me  me.countcry=me.countcry-1      if me.countcry=0 then    me.mStop(me.SCRY)      end if  endon mStop me,thesound  if sound(thesound).isBusy() then    sound(thesound).fadeOut(me.array[thesound].thefadeout)  end ifend